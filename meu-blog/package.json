{
  "name": "leonciolopes",
  "private": true,
  "version": "1.0.0",
  "homepage": "https://leonciolopes2528.github.io/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "node -e \"require('fs').copyFileSync('dist/index.html','dist/404.html')\"",
    "build:snap": "vite build && node -e \"require('fs').copyFileSync('dist/index.html','dist/404.html')\" && react-snap",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "reactSnap": {
    "source": "dist",
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/",
      "/sobre",
      "/mandato",
      "/mandato/pautas",
      "/mandato/projetos-de-lei",
      "/mandato/fiscalizacoes",
      "/eventos",
      "/contato"
    ]
  },
  "dependencies": {
    "lucide-react": "^0.452.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.18",
    "vite": "^5.4.0"
  }
}
